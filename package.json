{
  "name": "studioflow-workflow",
  "version": "1.0.0",
  "description": "StudioFlow by Meisner Interiors - Interior Design Project Workflow Management System",
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate --no-engine && next build",
    "build:vercel": "prisma generate --no-engine && next build",
    "build:db": "prisma db push --accept-data-loss && npm run build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "db:push": "prisma db push --accept-data-loss",
    "db:push:prod": "prisma db push --accept-data-loss --skip-generate",
    "db:studio": "prisma studio",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:seed": "tsx prisma/seed.ts",
    "build:full": "prisma db push --accept-data-loss && prisma generate && next build",
    "backup": "node scripts/backup-prisma.js",
    "backup:simple": "node scripts/simple-backup.js",
    "backup:full": "node scripts/backup-prisma.js",
    "backup:emergency": "node scripts/simple-backup.js",
    "backup:restore": "node scripts/restore-prisma.js",
    "backup:pg": "node scripts/backup-database.js",
    "backup:restore:pg": "node scripts/restore-database.js",
    "backup:full:prisma": "node scripts/prisma-backup.js",
    "backup:schedule": "schtasks /create /tn \"ResidentOne-DailyBackup\" /tr \"powershell.exe -ExecutionPolicy Bypass -WindowStyle Hidden -File 'C:\\Users\\ADMIN\\Desktop\\residentone-workflow\\scripts\\daily-backup.ps1'\" /sc daily /st 02:00 /f",
    "backup:test": "schtasks /run /tn \"ResidentOne-DailyBackup\"",
    "backup:manual": "powershell -ExecutionPolicy Bypass -File scripts/daily-backup.ps1",
    "backup:list": "dir backups\\database-backup-*.* /O-D",
    "dev:safe": "npm run backup && npm run dev",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "type-check": "tsc --noEmit"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
