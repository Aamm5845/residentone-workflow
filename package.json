{
  "name": "studioflow-workflow",
  "version": "1.0.0",
  "description": "StudioFlow by Meisner Interiors - Interior Design Project Workflow Management System",
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
