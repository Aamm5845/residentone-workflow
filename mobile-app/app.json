{
  "expo": {
    "name": "ResidentOne Mobile",
    "slug": "residentone-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "residentone",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.residentone.mobile"
    },
    "android": {
      "package": "com.residentone.mobile"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      "expo-asset"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
